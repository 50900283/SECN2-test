cat > /tmp/config.html << EOF

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<head>
	<title>SECN Configuration</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<style type="text/css">
	@import url(/lib/configstyle.css); 
	</style>
	<script type="text/javascript" src="/lib/jquery-latest.js"></script>
	<script type="text/javascript" src="/lib/jquery.validate.js"></script>
	<script type="text/javascript" src="/lib/checkip.js"></script>
	<script type="text/javascript" src="/lib/tabs.js"></script>
</head>

<body>
<div class="section">
	<div class="topbar">
		<img src="/images/vtlogo.png" alt="Village Telco">
		<div class="titletext">	
